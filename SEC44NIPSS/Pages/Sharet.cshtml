@page
@model SEC44NIPSS.Pages.SharetModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>SEC 44 TimeTable for @TempData["date"]</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="">

    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="~/dashboard/bower_components/bootstrap/dist/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/dashboard/bower_components/font-awesome/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/dashboard/bower_components/Ionicons/css/ionicons.min.css">
    <!-- DataTables -->
    <link rel="stylesheet" href="~/dashboard/bower_components/datatables.net-bs/css/dataTables.bootstrap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/dashboard/dist/css/AdminLTE.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/dashboard/dist/css/skins/_all-skins.min.css">
    <link rel="shortcut icon" href="~/img/download.png">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="https://cdn.jsdelivr.net/npm/summernote@0.8.18/dist/summernote.min.css" rel="stylesheet">
    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-X0CM3VESSW"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'G-X0CM3VESSW');
    </script>

    <meta property="og:site_name" content="SEC 44 TimeTable for @TempData["date"]">
    <meta property="og:title" content="SEC 44 TimeTable for @TempData["date"]" />
    <meta property="og:description" content="TimeTable Schedule" />
    <meta property="og:image" itemprop="image" content="https://sec44nipss.com/assets/images/logo-short.png">
    <meta property="og:type" content="website" />
    <meta property="og:updated_time" content="@DateTime.UtcNow.AddHours(1)" />


</head>
<body>
    <div class="container">
        <div class="row">
            <div class="col-xl-12" style="font-weight:700;font-size:16px;">

                <!-- Traffic sources -->
                <div class="box" style="margin:5px;">
                    <div class="box">
                        <div class="box-header">
                            <a asp-page="/Dashboard/Voting" class="btn btn-danger pull-right">Go Back</a>
                        </div>
                        <div class="box-header">
                            <h1 style="text-align:center;">THE NATIONAL INSTITUTE (SEC 44)</h1>
                            <h2 class="box-title" style="text-align:center;">@TempData["date"] </h2>
                            <a href="whatsapp://send?text=@TempData["link"]" data-action="share/whatsapp/share"
                               target="_blank" class="btn btn-primary btn-lg"> Share to WhatsApp </a>
                            
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>