@page
@model SEC44NIPSS.Pages.Web.projectxModel
@{
}


<div class="rs-breadcrumbs img10">
    <div class="container">
        <div class="breadcrumbs-inner">
            <h1 class="page-title">
                LEGACY PROJECT for @Model.Alumni.Title
                <span class="watermark">Syndicate</span>
            </h1>
            <span class="sub-text">

            </span>
        </div>
    </div>
</div>
<!-- Breadcrumbs End -->

<div class="rs-about style2 md-pt-70 md-pb-70" style="padding:30px 0 10px 0;">
    <div class="container">
        <div class="sec-title3 text-center md-mb-45" style="">
            <h2 class="title" style="border: 2px solid #ffffff; box-shadow: 0px 1.6px 3.6px rgb(0 0 0 / 13%), 0px 0px 2.9px rgb(0 0 0 / 11%); margin: 30px 5px 5px 5px; ">
                <span style="color:#36b43d;">@Model.Alumni.GeneralTopic</span>

            </h2>
            @if (Model.SecProject != null)
            {
        <img src="@Model.SecProject.Image" style="margin:0 auto;text-align:center;" />
            }


                <span class="sub-title" style="font-size: 33px; font-weight: 800; border-radius: 4px;padding-top:20px;margin-top:40px;">COMMUTTE</span>

            </div>
        
    </div>

</div>

<!-- Team Section Start -->
<div class="rs-team style1 team-style1 pt-100 pb-100 md-pt-70 md-pb-70">
    <div class="container">
        @if (Model.SecProject != null)
        {
        <div class="row">
            
            @foreach (var item in Model.SecProjectExecutives)
            {
            <div class="col-md-4">
                <div style="margin: 4px; box-shadow: 2px 1.6px 3.6px rgb(81 172 120), 2px 0px 2.9px rgb(81 172 120), 2px 0px 2.9px rgb(81 172 120), 2px 0px 2.9px rgb(81 172 120) ">
                    @if (!String.IsNullOrEmpty(item.Profile.AboutProfile))
                    {

                        <div class="" style="height: 500px; background: url('@item.Profile.AboutProfile'); background-repeat: no-repeat; background-size: cover;background-position: center; ">

                        </div>
                    }
                    else
                    {

                        <div class="" style="height: 500px; background: url('../img/adamm.jpg'); background-repeat: no-repeat; background-size: cover; background-position: center; ">

                        </div>
                    }
                    <div class="card-body" style="padding: 5px; height: 80px; background-color: #52ad79; color: white;">

                        <h6 class="card-title text-center uppercase" style="font-size:15px;font-weight:900;color:#fff;">@item.Profile.Title @item.Profile.FullName</h6>
                        <h6 class="card-title text-center uppercase" style="font-size: 12px; color: #fff;">
                            @item.Position
                        </h6>
                       
                    </div>
                </div>
            </div>
            }

        </div>
        }</div>
</div>


