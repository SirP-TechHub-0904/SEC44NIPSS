@page
@model SEC44NIPSS.Pages.Web.AlumniModel

@{
    ViewData["Title"] = "Alumni";
}

<div class="rs-breadcrumbs img10">
    <div class="container">
        <div class="breadcrumbs-inner">
            <h1 class="page-title">
                ALUMNI 
                <span class="watermark">Syndicate</span>
            </h1>
            <span class="sub-text">

            </span>
        </div>
    </div>
</div>
<!-- Breadcrumbs End -->
<!-- Team Section Start -->
<div class="rs-about style2 md-pt-70 md-pb-70" style="padding:30px 0 10px 0;">
    <div class="container">
        
        <div class="row">
            @foreach (var x in Model.Alumni)
            {


            <div class="col-md-6" style="margin:10px 0">
                <a asp-page="/Web/AlumniSection" asp-route-id="@x.Id">
                    <div class="sec-title" style="border: 2px solid #ffffff; box-shadow: 1px 1.6px 3.6px rgb(30 64 44), 2px 0px 2.9px rgb(65 112 86); padding: 16px; background-color: @x.Color; height: 185px;">

                        <h3 style="text-align:center;color:#000000;margin:0;">@x.Title</h3>
                        <h6 style="text-align: center; color: #000000;text-shadow: 1px 1px 1px #ff6a00;">@x.GeneralTopic</h6>
                    </div>
                </a>
            </div>
            }

        </div>
    </div>

</div>
